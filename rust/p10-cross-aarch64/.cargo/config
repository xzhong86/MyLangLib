[build]
target = "aarch64-unknown-linux-gnu"  # dynamically linked
#target = "aarch64-unknown-linux-gnu"  # statically linked

[target.aarch64-unknown-linux-gnu]
linker = "aarch64-linux-gnu-gcc-12"  # install gcc-12-aarch64-linux-gnu first

[target.aarch64-unknown-linux-musl]
linker = "aarch64-linux-gnu-ld"  # install bintutils-aarch64-linux-gnu if needed

